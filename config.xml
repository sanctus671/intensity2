    <platform name="windows">
        <preference name="windows-appx-target" value="8.1-phone" />
        <preference name="WindowsToastCapable" value="true" />
        <Preference name="phonegap-version" value="cli-5.4.1" />
        <preference name="SplashScreenBackgroundColor" value="0xFFFFFFFF" />
    </platform>
    <icon src="resources/icon.png" />
    <splash src="resources/splash.png" />
    <engine name="android" spec="~6.3.0" />
    <plugin name="cordova-plugin-ionic-webview" spec="^1.1.16" />
    <gap:plugin spec="https://github.com/brodybits/cordova-sqlite-legacy-build-support" />
    <gap:plugin name="cordova-plugin-facebook4" source="npm" version="1.7.4">
        <param name="APP_ID" value="414561158706027" />
        <param name="APP_NAME" value="Intensity" />
    </gap:plugin>
