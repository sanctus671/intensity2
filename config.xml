        <resource-file src="resources/android/icon/mipmap-hdpi/ic_launcher_foreground.png" target="res/mipmap-hdpi-v4/ic_launcher_foreground.png" />
        <resource-file src="resources/android/icon/mipmap-mdpi/ic_launcher.png" target="res/mipmap-mdpi-v4/ic_launcher.png" />
        <resource-file src="resources/android/icon/mipmap-mdpi/ic_launcher_round.png" target="res/mipmap-mdpi-v4/ic_launcher_round.png" />
        <resource-file src="resources/android/icon/mipmap-mdpi/ic_launcher_foreground.png" target="res/mipmap-mdpi-v4/ic_launcher_foreground.png" />
        <resource-file src="resources/android/icon/mipmap-xhdpi/ic_launcher.png" target="res/mipmap-xhdpi-v4/ic_launcher.png" />
        <resource-file src="resources/android/icon/mipmap-xhdpi/ic_launcher_round.png" target="res/mipmap-xhdpi-v4/ic_launcher_round.png" />
        <resource-file src="resources/android/icon/mipmap-xhdpi/ic_launcher_foreground.png" target="res/mipmap-xhdpi-v4/ic_launcher_foreground.png" />
        <resource-file src="resources/android/icon/mipmap-xxhdpi/ic_launcher.png" target="res/mipmap-xxhdpi-v4/ic_launcher.png" />
        <resource-file src="resources/android/icon/mipmap-xxhdpi/ic_launcher_round.png" target="res/mipmap-xxhdpi-v4/ic_launcher_round.png" />
        <resource-file src="resources/android/icon/mipmap-xxhdpi/ic_launcher_foreground.png" target="res/mipmap-xxhdpi-v4/ic_launcher_foreground.png" />
        <resource-file src="resources/android/icon/mipmap-xxxhdpi/ic_launcher.png" target="res/mipmap-xxxhdpi-v4/ic_launcher.png" />
        <resource-file src="resources/android/icon/mipmap-xxxhdpi/ic_launcher_round.png" target="res/mipmap-xxxhdpi-v4/ic_launcher_round.png" /> 
        <resource-file src="resources/android/icon/mipmap-xxxhdpi/ic_launcher_foreground.png" target="res/mipmap-xxxhdpi-v4/ic_launcher_foreground.png" />       
        
        
        
        
        <resource-file src="resources/android/styles.xml" target="res/values/styles.xml" />

        <resource-file src="resources/android/icon/mdpi_small_icon.png" target="res/drawable-mdpi-v4/small_icon.png" />
        <resource-file src="resources/android/icon/hdpi_small_icon.png" target="res/drawable-hdpi-v4/small_icon.png" />
        <resource-file src="resources/android/icon/xhdpi_small_icon.png" target="res/drawable-xhdpi-v4/small_icon.png" />
        <resource-file src="resources/android/icon/xxhdpi_small_icon.png" target="res/drawable-xxhdpi-v4/small_icon.png" />
        <resource-file src="resources/android/icon/xxxhdpi_small_icon.png" target="res/drawable-xxxhdpi-v4/small_icon.png" />                
    </platform>
    <splash platform="android" density="port-ldpi" src="resources/android/splash/splash-port-ldpi.png" />
    <splash platform="android" density="port-mdpi" src="resources/android/splash/splash-port-mdpi.png" />
    <splash platform="android" density="port-hdpi" src="resources/android/splash/splash-port-hdpi.png" />
    <splash platform="android" density="port-xhdpi" src="resources/android/splash/splash-port-xhdpi.png" />
    <splash platform="android" density="port-xxhdpi" src="resources/android/splash/splash-port-xxhdpi.png" />
    <splash platform="android" density="port-xxxhdpi" src="resources/android/splash/splash-port-xxxhdpi.png" />
    <splash platform="android" density="land-ldpi" src="resources/android/splash/splash-land-ldpi.png" />
    <splash platform="android" density="land-mdpi" src="resources/android/splash/splash-land-mdpi.png" />
    <splash platform="android" density="land-hdpi" src="resources/android/splash/splash-land-hdpi.png" />
